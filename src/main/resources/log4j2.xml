<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="MAIN" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t] %-5p %c{1}:%L - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="MAIN"/>
        </Root>
    </Loggers>
</Configuration>
